{
  "family": "fargate-java21-task",
  "executionRoleArn": "arn:aws:iam::523919781340:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::523919781340:role/ecsTaskRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "containerDefinitions": [
    {
      "name": "fargate-java21-demo",
      "image": "523919781340.dkr.ecr.us-west-1.amazonaws.com/fargate-java21-demo:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "fargate-java21-demo",
          "awslogs-region": "us-west-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
